{
  "name": "patternfly-org-4",
  "private": true,
  "version": "4.1.29",
  "author": "Red Hat",
  "license": "MIT",
  "scripts": {
    "generate": "node scripts/sourceMD.js",
    "analyze": "yarn build --env analyze",
    "build": "yarn generate && yarn build:server && yarn build:client",
    "build:server": "webpack --mode production --config webpack.server.config.js",
    "build:client": "webpack --mode production",
    "clean": "rm -rf .cache public size-plugin.json src/generated",
    "develop": "yarn generate && webpack-dev-server --mode development",
    "test:a11y": "patternfly-a11y --file .cache/fullscreenPages.json --prefix http://localhost:9000/v4"
  },
  "dependencies": {
    "@patternfly/react-docs": "5.8.19",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "theme-patternfly-org": "0.0.1"
  },
  "devDependencies": {
    "eslint-config-react-app": "^5.0.2",
    "fs-extra": "^9.0.0",
    "glob": "^7.1.6"
  }
}
