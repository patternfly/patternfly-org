  "private": false,
  "version": "1.0.0",
  "author": "Red Hat",
  "scripts": {
    "develop": "(shx --silent rm -rf public || shx true) && gatsby develop",
    "build": "yarn repos && gatsby build --prefix-paths",
    "repos": "yarn pf-core && yarn react-core",
    "pf-core": "node ./scripts/pf-core.js",
    "react-core": "yarn react-core-src && yarn react-core-docs",
    "react-core-src": "node ./scripts/react-core-src.js",
    "react-core-docs": "node ./scripts/react-core-docs.js",
    "serve": "gatsby serve",
    "start": "node server.js",
    "gh-pages": "gatsby build --prefix-paths && gh-pages -d public",
    "lint": "node_modules/.bin/eslint src --fix",
    "server": "cross-env NODE_ENV=development DEBUG=api nodemon server.js"
  },
